{
  "expo": {
    "name": "M1A",
    "slug": "m1a",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "m1a",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.merkabaent.m1a",
      "buildNumber": "14",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "M1A needs access to your photo library to upload profile photos and cover images.",
        "NSCameraUsageDescription": "M1A needs access to your camera to take photos for your profile.",
        "NSPhotoLibraryAddUsageDescription": "M1A needs access to save photos to your library.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "usesAppleSignIn": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:m1a.app",
        "applinks:www.m1a.app"
      ],
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "package": "com.merkabaent.m1a",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.RECORD_AUDIO"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "meta": {
        "og:image": "./assets/images/splash-icon.png",
        "twitter:image": "./assets/images/splash-icon.png",
        "twitter:card": "summary_large_image"
      }
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "M1A needs access to your photos to upload profile and cover images.",
          "cameraPermission": "M1A needs access to your camera to take photos."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#9C27B0",
          "sounds": []
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "lancemakesmusic",
    "extra": {
      "eas": {
        "projectId": "5768f0a5-467f-4215-b351-84636ca67414"
      }
    }
  }
}
